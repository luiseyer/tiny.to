{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "semi": false,
  "printWidth": 100,
  "tailwindStylesheet": "./src/app.css",
  "tailwindFunctions": ["cn"],
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteSortOrder": "options-scripts-markup-styles"
      }
    }
  ]
}
